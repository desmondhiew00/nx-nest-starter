  "private": true,
  "scripts": {
    "prepare": "husky install",
    "postinstall": "cd ./libs/plugin && yarn install",
    "prisma:generate": "rimraf libs/generated/src/graphql || prisma generate && npx nx run plugin:parse-gql-schema",
    "prisma:migration:gen": "npx prisma migrate dev --create-only ",
    "prisma:migration:run": "npx prisma migrate dev && npm run prisma:generate",
    "prisma:seed": "npx nx run plugin:prisma-seed",
    "query:generate": "npx nx generate query-generator",
    "parse-gql-schema": "npx nx run plugin:parse-gql-schema && npm run format",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format --write",
    "dev:demo": "npx nx reset && npx nx serve demo"
  },
